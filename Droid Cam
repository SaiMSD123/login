	var modal = document.getElementById('variant');

	var now = new Date();
	var then = localStorage.variant ? Date.parse(localStorage.variant) : 0;
	if ((now - then) > 1309630999) modal.style.display = 'block';

	function save() {
		modal.style.display = 'none';
		localStorage['variant'] = now;
	}
